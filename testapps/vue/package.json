{
  "name": "@tolgee/vue-testapp",
  "version": "3.2.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve --skip-plugins @vue/cli-plugin-eslint",
    "develop": "vue-cli-service serve --skip-plugins @vue/cli-plugin-eslint",
    "build": "vue-cli-service build --skip-plugins @vue/cli-plugin-eslint",
    "build:e2e": "VUE_APP_TOLGEE_API_URL=http://localhost:8202 VUE_APP_TOLGEE_API_KEY=examples-admin-imported-project-implicit vue-cli-service build --skip-plugins @vue/cli-plugin-eslint --dest dist-e2e",
    "clean": "rm -rf build build-e2e dist dist-e2e lib",
    "lint": "vue-cli-service lint",
    "serve": "serve -p 3002 -s dist",
    "serve:dev": "serve -p 3002 -s dist-e2e"
  },
  "dependencies": {
    "@tolgee/core": "^3.2.0",
    "@tolgee/ui": "^3.2.0",
    "@tolgee/vue": "^3.2.0",
    "core-js": "^3.21.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.2.29",
    "prettier": "^2.5.1",
    "serve": "^13.0.2",
    "typescript": "~4.5.5"
  }
}
