{
  "name": "@tolgee/svelte-testapp",
  "version": "4.4.0",
  "private": true,
  "scripts": {
    "dev": "svelte-kit dev",
    "develop": "svelte-kit dev",
    "build": "svelte-kit build",
    "build:e2e": "SVELTE_TESTAPP_DIST_DIR=./build-e2e VITE_TOLGEE_API_URL=http://localhost:8202 VITE_TOLGEE_API_KEY=examples-admin-imported-project-implicit svelte-kit build",
    "clean": "rm -rf build build-e2e dist dist-e2e lib .svelte-kit",
    "preview": "svelte-kit preview",
    "serve": "serve ./build",
    "serve:e2e": "serve ./build-e2e",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "1.0.0-next.23",
    "@sveltejs/kit": "1.0.0-next.206",
    "@tolgee/core": "^4.4.0",
    "@tolgee/svelte": "^4.4.0",
    "@tolgee/ui": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.6.0",
    "serve": "^12.0.1",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.4",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "type": "module"
}
