{
  "name": "@tolgee/svelte-testapp",
  "version": "4.8.1",
  "private": true,
  "scripts": {
    "dev": "npm run develop",
    "develop": "vite dev --port 5000",
    "build": "vite build",
    "build:e2e": "SVELTE_TESTAPP_DIST_DIR=./build-e2e VITE_TOLGEE_API_URL=http://localhost:8202 VITE_TOLGEE_API_KEY=examples-admin-imported-project-implicit vite build",
    "clean": "rm -rf build build-e2e dist dist-e2e lib .svelte-kit",
    "preview": "vite preview",
    "serve": "serve -p 5000 ./build",
    "serve:e2e": "serve -p 5000 ./build-e2e",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --cache --check . && eslint .",
    "format": "prettier --cache --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "1.0.0-next.39",
    "@sveltejs/kit": "1.0.0-next.429",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@tolgee/core": "^4.8.0",
    "@tolgee/svelte": "^4.8.0",
    "@tolgee/ui": "^4.8.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  },
  "type": "module"
}
