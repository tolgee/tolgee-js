    "@testing-library/vue": "^6.4.2",
    "@tolgee/ui": "^2.5.3",
    "@types/jest": "^27.0.2",
    "@vue/vue3-jest": "^27.0.0-alpha.3",
    "concurrently": "6.3.0",
    "jest": "^27.3.1",
    "rollup": "2.59.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-vue": "^6.0.0",
