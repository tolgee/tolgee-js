{
  "name": "@tolgee/svelte",
  "version": "5.16.2",
  "description": "Svelte implementation for Tolgee localization framework",
  "repository": "https://github.com/tolgee/tolgee-js",
  "homepage": "https://tolgee.io",
  "keywords": [
    "localization",
    "internationalization",
    "l10n",
    "i18n",
    "translation",
    "languages"
  ],
  "scripts": {
    "dev": "npm run develop",
    "develop": "sleep 5 && nodemon --exec svelte-package",
    "build": "svelte-kit sync && svelte-package",
    "clean": "rm -rf build build-e2e dist dist-e2e lib .svelte-kit package",
    "serve": "serve -s dist",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "jest src --collect-coverage",
    "lint": "prettier --cache --check . && eslint .",
    "format": "prettier --cache --write ."
  },
  "author": "JanCizmar",
  "license": "MIT",
  "peerDependencies": {
    "svelte": "^3.29.0 || ^4.0.0"
  },
  "dependencies": {
    "@tolgee/web": "5.16.2"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "1.0.2",
    "@sveltejs/kit": "1.2.6",
    "@sveltejs/package": "1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@tolgee/format-icu": "5.16.2",
    "@tolgee/testing": "5.16.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "^28.1.7",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "28.1.2",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^5.0.1",
    "svelte2tsx": "^0.6.0",
    "ts-jest": "^28.0.8",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "type": "module",
  "publishConfig": {
    "directory": "./package",
    "access": "public"
  },
  "sideEffects": false
}
