{
  "name": "@tolgee/svelte",
  "version": "4.8.0",
  "scripts": {
    "dev": "npm run develop",
    "develop": "sleep 5 && nodemon --exec svelte-package",
    "build": "svelte-kit sync && svelte-package",
    "clean": "rm -rf build build-e2e dist dist-e2e lib .svelte-kit",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "jest src",
    "lint": "prettier --cache --check . && eslint .",
    "format": "prettier --cache --write ."
  },
  "dependencies": {
    "@tolgee/core": "^4.8.0",
    "@tolgee/ui": "^4.8.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "1.0.0-next.65",
    "@sveltejs/kit": "1.0.0-next.429",
    "@sveltejs/package": "1.0.0-next.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.1",
    "@types/jest": "^28.1.7",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "28.1.2",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.14",
    "ts-jest": "^28.0.8",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.9"
  },
  "type": "module",
  "publishConfig": {
    "directory": "./package",
    "access": "public"
  }
}
