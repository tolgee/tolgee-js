{
  "name": "@tolgee/react",
  "version": "5.16.2",
  "description": "React implementation for Tolgee localization framework",
  "main": "./dist/tolgee-react.cjs.js",
  "module": "./dist/tolgee-react.esm.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/tolgee/tolgee-js",
  "homepage": "https://tolgee.io",
  "keywords": [
    "localization",
    "internationalization",
    "l10n",
    "i18n",
    "translation",
    "languages"
  ],
  "declaration": true,
  "directories": {
    "lib": "lib",
    "dist": "dist"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js && tsc --project tsconfig.prod.json",
    "clean": "rm -rf build build-e2e dist dist-e2e lib",
    "test": "jest --collect-coverage",
    "develop": "concurrently \"rollup -c rollup.config.js -w\" \"tsc --watch --preserveWatchOutput --project tsconfig.prod.json\" "
  },
  "author": "JanCizmar",
  "license": "MIT",
  "files": [
    "index.js",
    "lib/**/*",
    "dist/**/*",
    "src/**/*"
  ],
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.1 || ^18.1.0"
  },
  "dependencies": {
    "@tolgee/web": "5.16.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "8.3.4",
    "@testing-library/dom": "^8.7.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^14.0.0",
    "@tolgee/format-icu": "5.16.2",
    "@tolgee/testing": "5.16.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.8",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "concurrently": "7.2.2",
    "jest": "^27.2.4",
    "jest-fetch-mock": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.3",
    "rollup": "2.78.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "^27.0.5",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./dist/tolgee-react.cjs.js",
      "module": "./dist/tolgee-react.esm.js",
      "import": "./dist/tolgee-react.esm.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
