{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    ".env*"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "lib/**",
        "package/**",
        "build/**",
        ".next/**",
        ".angular/**",
        ".svelte-kit/**"
      ]
    },
    "build:e2e": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "dist/**",
        "lib/**",
        "package/**",
        "build/**",
        ".next/**",
        ".angular/**",
        ".svelte-kit/**",
        "dist-e2e/**",
        "build-e2e/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ]
    },
    "develop": {
      "dependsOn": [
        "^develop"
      ],
      "cache": false
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "cache": false
    },
    "clean": {
      "dependsOn": [],
      "cache": false
    }
  }
}
